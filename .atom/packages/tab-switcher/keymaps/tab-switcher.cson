"atom-workspace":
  "alt-[": "tab-switcher:previous"
  "alt-[ ^alt": "tab-switcher:select"
  "alt-]": "tab-switcher:next"
  "alt-] ^alt": "tab-switcher:select"

"ol.tab-switcher-tab-list":
  "^alt": "tab-switcher:select"
  "alt-up": "tab-switcher:previous"
  "alt-down": "tab-switcher:next"
  "alt-escape": "tab-switcher:cancel"
  "alt-n": "tab-switcher:next"
  "alt-p": "tab-switcher:previous"
  "alt-w": "tab-switcher:close"
  "alt-s": "tab-switcher:save"
